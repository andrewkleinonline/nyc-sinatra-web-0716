<h1>Edit the Landmark:</h1>
<form class="" action="/landmarks/<%= @landmark.id %>" method="post">
  <input type="hidden" name="_method" value="patch">
  <h3>Name: <input id="name" type="text" name="landmark[name]" value="<%= @landmark.name %>"></h3>
  <h3>Year Completed: <input id="year_completed" type="text" name="landmark[year_completed]" value="<%= @landmark.year_completed %>"></h3>
  <input type="submit" name="submit" value="Edit Landmark">
</form>
